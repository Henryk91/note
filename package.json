    "babel-jest": "^30.0.5",
    "babel-loader": "^9.2.1",
    "body-parser": "^1.20.3",
    "browserslist": "^4.23.3",
    "caniuse-lite": "^1.0.30001646",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.2.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.39.0",
    "cors": "^2.8.5",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "file-loader": "^6.2.0",
