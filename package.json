{
  "name": "note-list",
  "version": "2.0.0",
  "description": "Note list App with a node mongo backend",
  "main": "build/server/index.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node --trace-warnings build/server/index.js",
    "start:all": "npm run build && node --trace-deprecation build/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "npm run test:integration",
    "test:integration": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json mocha --timeout 20000 -r ts-node/register/transpile-only --file __tests__/integration/setup.ts \"__tests__/integration/**/*.test.ts\""
  },
  "author": "Henry Koekemoer",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^30.0.5",
    "babel-loader": "^9.2.1",
    "bcryptjs": "^3.0.2",
    "browserslist": "^4.23.3",
    "caniuse-lite": "^1.0.30001646",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.2.0",
    "cookie-parser": "^1.4.7",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.39.0",
    "cors": "^2.8.5",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-jwt": "^8.5.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "file-loader": "^6.2.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "html-webpack-plugin": "^5.6.3",
    "jsonwebtoken": "^9.0.2",
    "marked": "^16.1.2",
    "mongodb": "^6.9.0",
    "mongoose": "^8.17.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.7",
    "react": "^16.14.0",
    "react-animated-weather": "^2.0.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^4.3.1",
    "regenerator-runtime": "^0.14.1",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^7.3.0",
    "xss": "^1.0.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.3.11",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mocha": "^10.0.10",
